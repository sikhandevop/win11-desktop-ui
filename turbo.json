{
	"$schema": "https://turborepo.org/schema.json",
	"globalDependencies": ["**/.env"],
	"pipeline": {
		"win": {},
		"topo": {
			"dependsOn": ["^topo"]
		},
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["build/**"]
		},
		"dev": {
			"persistent": true,
			"cache": false
		},
		"start": {
			"persistent": true,
			"cache": false
		},
		"format": {
			"outputs": ["node_modules/.cache/.prettiercache"],
			"outputMode": "new-only"
		},
		"lint": {
			"dependsOn": ["^topo"],
			"outputs": ["node_modules/.cache/.eslintcache"]
		},
		"typecheck": {
			"dependsOn": ["^topo"],
			"outputs": ["node_modules/.cache/tsbuildinfo.json"]
		},
		"clean": {
			"cache": false
		},
		"//#clean": {
			"cache": false
		}
	},
	"globalEnv": []
}
