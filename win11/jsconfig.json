{
  "compilerOptions": {
   "jsx": "react-jsx",
    "target": "esnext",
    "lib": ["dom", "esnext"],
    "skipLibCheck": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
     "@src/*": ["./src/*"]
    },     
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"],
}
